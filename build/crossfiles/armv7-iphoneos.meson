[binaries]
c = 'clang'
cpp = 'clang++'
ar = 'ar'
strip = 'strip'

[built-in options]
c_args = ['-arch', 'armv7', '-miphoneos-version-min=10.0', '-isysroot', iossdkroot]
cpp_args = c_args
c_link_args = c_args
cpp_link_args = c_args

[properties]
root = iosroot
has_function_printf = true
has_function_hfkerhisadf = false

[host_machine]
system = 'darwin'
cpu_family = 'arm'
cpu = 'armv7'
endian = 'little'
